  "version": "1.0.0",
  "description": "TritonHTTP Web Server",
  "main": "index.js",
  "scripts": {
    "test": "jest testing --config=jest.config.js",
    "test:setup": "npm run go:setup",
    "test:start-server": "GOPATH=$(pwd) PATH=$PATH:$GOPATH/bin go run src/main/main.go",
    "lint": "eslint testing/**/*.js --fix",
    "go:setup": "GOPATH=$(pwd) PATH=$PATH:$GOPATH/bin go get github.com/go-ini/ini",
    "start": "npm run go:setup && GOPATH=$(pwd) PATH=$PATH:$GOPATH/bin go run src/main/main.go src/myconfig.ini"
  },
  "testing": {
    "use-default-server": true,
    "server-port": 8080,
    "doc-root": "./sample_htdocs/"
  },
